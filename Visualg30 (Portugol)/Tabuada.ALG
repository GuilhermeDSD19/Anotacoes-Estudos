Algoritmo "Somador"


Var
   cont, n, r : inteiro

Inicio
       Escreva ("De qual numero você quer ver a tabuada? ")
       Leia (n)
       cont <- 1
       Repita
             r <- n * cont
             Escreval (n, " X", cont, " = ", r)
             cont <- cont + 1
       Ate (cont > 10)




Fimalgoritmo